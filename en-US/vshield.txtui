        <xlf:target state="translated">Deny access</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="24" resname="IDS_SCANSYSBOOT">
        <xlf:target state="translated">Early boot scan</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="25" resname="IDS_SHIELD_PRESENTATION">
        <xlf:target state="translated">&lt;#ProductName#&gt; Shield actively protects your device from threats by detecting and removing malicious software.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="26" resname="IDS_AFTER_SCAN_ACTIONS">
        <xlf:target state="translated">Threat actions</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="27" resname="IDS_RESET">
        <xlf:target state="translated">Reset advanced settings</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="28" resname="IDS_RESET_DESCRIPTION">
        <xlf:target state="translated">This will reset your advanced settings to default.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="29" resname="IDS_RESET_CONFIRMATION">
        <xlf:target state="translated">Are you sure you want to reset your &lt;#ProductName#&gt; Shield settings to default?</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="30" resname="IDS_ANTIVIRUS_PAD_TOOLTIP">
        <xlf:target state="translated">Real-time protection blocks any threat from running on your device.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="31" resname="IDS_SCAN_APPLICATIONS_DESCRIPTION">
        <xlf:target state="translated">Scans only accessed applications for threats that may compromise your device.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="32" resname="IDS_SCAN_PUA_DESCRIPTION">
        <xlf:target state="translated">Scans for undesirable applications such as adware or spyware.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="33" resname="IDS_SCAN_SCRIPTS_DESCRIPTION">
        <xlf:target state="translated">Scans different applications for scripts that might harm your device.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="34" resname="IDS_SCAN_NETSH_DESCRIPTION">
        <xlf:target state="translated">Scan network paths when you access them to ensure the protection of your device.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="35" resname="IDS_SCAN_ARCHIVES_DESCRIPTION">
        <xlf:target state="translated">Scans for threats inside different types of archives.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="36" resname="IDS_SCANBOOT_DESCRIPTION">
        <xlf:target state="translated">Scans the device for threats that might alter the boot sequence.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="37" resname="IDS_NEWCHFILES_DESCRIPTION">
        <xlf:target state="translated">Scans only files that are new on your device or files that have been modified.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="38" resname="IDS_SCAN_KEYLOG_DESCRIPTION">
        <xlf:target state="translated">Scans the device for keyloggers that may capture personal data such as passwords.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="39" resname="IDS_SCANSYSBOOT_DESCRIPTION">
        <xlf:target state="translated">Scans your device at startup as soon as all critical services are loaded.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="40" resname="IDS_AFTER_SCAN_ACTIONS_DESCRIPTION">
        <xlf:target state="translated">Automatic actions to be taken on found threats.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="41" resname="IDS_UPGRADE_TO_UNLOCK">
        <xlf:target state="translated">Upgrade to unlock this feature</xlf:target>
      </xlf:trans-unit>
