      </xlf:trans-unit>
      <xlf:trans-unit id="6" resname="IDS_ACTIVATE_FEATURE">
        <xlf:target state="translated">Activate protection</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="7" resname="IDS_SHIELD_TITLE">
        <xlf:target state="translated">Shield</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="8" resname="IDS_MB_TEXT">
        <xlf:target state="translated">MB</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="9" resname="IDS_REALTIME_PROTECTION">
        <xlf:target state="translated">&lt;#ProductName#&gt; Shield</xlf:target>
      </xlf:trans-unit>
