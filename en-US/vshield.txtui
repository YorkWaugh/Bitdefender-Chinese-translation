        <xlf:target state="translated">Scan only applications</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="14" resname="IDS_SCAN_PUA">
        <xlf:target state="translated">Scan potentially unwanted applications</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="15" resname="IDS_SCAN_SCRIPTS">
        <xlf:target state="translated">Scan scripts</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="16" resname="IDS_SCAN_NETSH">
        <xlf:target state="translated">Scan network shares</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="17" resname="IDS_DISABLE_PROT_SECOND">
        <xlf:target state="translated">How long do you want to keep the protection disabled?</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="18" resname="IDS_SCANBOOT">
        <xlf:target state="translated">Scan boot sectors</xlf:target>
