        <xlf:target state="translated">Pause</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="11" resname="IDS_RESCAN">
        <xlf:target state="translated">Rescan</xlf:target>
      </xlf:trans-unit>
