  <xlf:file datatype="winres" original="CLWinRE" source-language="en" target-language="en-us">
    <xlf:body>
      <xlf:trans-unit id="1" resname="IDS_APP_TITLE">
        <xlf:target state="translated">Rescue Environment</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="2" resname="IDS_PREPARING_DESCRIPTION">
        <xlf:target state="translated">Preparing the Rescue Environment. This may take some time.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="3" resname="IDS_WAITING_FOR_SERVICE">
        <xlf:target state="translated">Rescue Environment services are starting...</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="4" resname="IDS_CONFIRMATION_DESCRIPTION">
        <xlf:target state="translated">You must restart your device in order to boot in Rescue Environment.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="5" resname="IDS_NO_RE_IMAGE_DESCRIPTION">
