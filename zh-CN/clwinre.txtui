        <xlf:target state="translated">Learn more</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="57" resname="IDS_TITLE_RESOLVED">
        <xlf:target state="translated">Detected threats were resolved!</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="58" resname="IDS_TITLE_SUCCESS">
        <xlf:target state="translated">Your system is clean!</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="59" resname="IDS_TITLE_ATTENTION">
        <xlf:target state="translated">Your system requires your attention!</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="60" resname="IDS_TITLE_WARNING">
        <xlf:target state="translated">Your system is infected!</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="61" resname="IDS_ACTION_RESCAN_DESCRIPTION">
        <xlf:target state="translated">Please rescan the files on which there was no action taken.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="62" resname="IDS_WINRE_SCAN_TYPE">
        <xlf:target state="translated">Rescue Environment Scan</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="63" resname="IDS_PASSWORD_DESCRIPTION">
        <xlf:target state="translated">The current file is a password-protected item. Please enter the password here below so that the Rescue Environment can scan it. If you do not know the password, you can choose to skip this file.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="64" resname="IDS_PASSWORD">
        <xlf:target state="translated">Password:</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="65" resname="IDS_SKIP_THIS_OBJECT">
        <xlf:target state="translated">Skip from scanning</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="66" resname="IDS_SKIP_PASSWORD_PROTECTED">
        <xlf:target state="translated">Skip all password-protected items without scanning them.</xlf:target>
      </xlf:trans-unit>
