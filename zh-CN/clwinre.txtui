      </xlf:trans-unit>
      <xlf:trans-unit id="69" resname="IDS_PASSWORD_SUGGESTION">
        <xlf:target state="translated">Rescue Environment found password-protected files that were skipped from scan. These files will be automatically scanned when extracted. Check the scan log for details.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="70" resname="IDS_EMAIL_ARCHIVES_TITLE">
        <xlf:target state="translated">Email archives</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="71" resname="IDS_EMAIL_ARCHIVES_SUGGESTION">
        <xlf:target state="translated">Rescue Environment found emails with malicious attachments that could not be cleaned. Don&apos;t worry, when you access any of these attachments the threat will be blocked to prevent any harm to your files or your device. Check the scan log for details.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="72" resname="IDS_CONFIRM_UNDERSTANDING">
        <xlf:target state="translated">OK, got it</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="73" resname="IDS_AFFECTED_ARCHIVE">
        <xlf:target state="translated">The archive containing this file also contains clean or possibly suspicious files. Move the archive to Quarantine.</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="74" resname="IDS_FILE_PATH">
        <xlf:target state="translated">File Path</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="75" resname="IDS_THREAT_NAME">
        <xlf:target state="translated">Threat Name</xlf:target>
      </xlf:trans-unit>
      <xlf:trans-unit id="76" resname="IDS_RESULT">
        <xlf:target state="translated">Result</xlf:target>
      </xlf:trans-unit>
